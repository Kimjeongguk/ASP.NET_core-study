@model Student

@{
    ViewBag.Title = "Edit";
}
<h3>학생정보수정</h3>
<form class="container" method="post">
    <div class="form-group">
        <div class="col-md-3">
            <label>Name:</label>
            <input type="text" asp-for="@Model.Id" class="form-control" disabled>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <label>이름:</label>
            <input type="text" asp-for="@Model.Name" class="form-control">
            <span asp-validation-for="@Model.Name"></span>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <label>나이:</label>
            <input type="text" asp-for="@Model.Age" class="form-control">
            <span asp-validation-for="@Model.Age"></span>

        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <label>국가:</label>
            <input type="text" asp-for="@Model.Country" class="form-control">
            <span asp-validation-for="@Model.Country"></span>

        </div>
    </div>
    <button type="submit" class="btn btn-primary">Update</button>

</form>